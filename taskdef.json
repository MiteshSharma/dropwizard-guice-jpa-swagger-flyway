  "executionRoleArn": "ECS_TASK_EXECUTION_ROLE",
  "containerDefinitions": [
    {
      "name": "mobile-server-app",
      "image": "<IMAGE1_NAME>",
      "cpu": 256,
      "portMappings": [
        {
